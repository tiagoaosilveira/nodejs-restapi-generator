module.exports = (sequelize, DataTypes) => {
    const {{ up name }} = sequelize.define('{{ up name }}', {
      name: DataTypes.STRING,
      quantity: DataTypes.INTEGER,
      price: DataTypes.FLOAT,
    },
    {
      timestamps: false,
      tableName: '{{ name }}s'
    });
  
    return {{ up name }};
}
